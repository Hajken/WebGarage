<div class="container">
    <div class="row well">
        <div class="col-xs-10 col-sm-10 col-md-6 col-xs-offset-1 col-sm-offset-1 col-md-offset-3">
            <div class="row">
                <div class="col-xs-6 col-sm-6 col-md-6">
                    <address>
                        <strong>Garage 2.0</strong>
                        <br>
                        Stockholmsvägen 213
                        <br>
                        Stockholm
                        <br>
                    </address>
                </div>
                <div class="col-xs-6 col-sm-6 col-md-6 text-right">
                    <p>
                        <em>Date: @ViewBag.DateNow</em>
                    </p>
                    <p>
                        <em>Receipt #: @ViewBag.VehicleId</em>
                    </p>
                </div>
            </div>
            <div class="row">
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <th>Product</th>
                            <th class="text-center">Minutes </th>
                            <th class="text-center">Price</th>
                            <th class="text-center">Total</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="col-md-9"><h4><em>Parking in Garage 2.0 </em></h4></td>
                            <td class="col-md-1" style="text-align: center"> @ViewBag.TotalTime </td>
                            <td class="col-md-1 text-center">$@ViewBag.PricePerMinut</td>
                            <td class="col-md-1 text-center">$@ViewBag.TotalPrice</td>
                        </tr>
                        <tr>
                            <td>   </td>
                            <td>   </td>
                            <td class="text-right"><h4><strong>Total: </strong></h4></td>
                            <td class="text-center text-danger"><h4><strong>$@ViewBag.TotalPrice</strong></h4></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
